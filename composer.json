{
  "name": "wexample/symfony-translations",
  "description": "A translations service overrides to support translations files includes and multiple domains in Symfony translations",
  "type": "library",
  "license": "MIT",
  "version": "1.0.62",
  "keywords": [
    "symfony-ux"
  ],
  "autoload": {
    "psr-4": {
      "Wexample\\SymfonyTranslations\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wexample\\SymfonyTranslations\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "weeger",
      "email": "romain.weeger@wexample.com"
    }
  ],
  "require": {
    "symfony/translation": ">=6.2",
    "wexample/symfony-helpers": "1.0.74",
    "wexample/symfony-testing": "1.0.62",
    "wexample/php-helpers": "1.0.79",
    "wexample/php-yaml": "1.0.47",
    "wexample/symfony-template": "0.0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "wexample/symfony-testing": "*"
  },
  "repositories": [
    {
      "type": "path",
      "url": "../*",
      "options": {
        "symlink": true
      }
    }
  ]
}